/*! ANIMATION FROM {{= it.animationName }} {{=it.maxDisplacement}}*/
@keyframes {{= it.animationName }} {
  0%  {
    transform: translateX(0);
  }
  100% {
    transform:  translateX({{=it.maxDisplacement}});
  }
}

@-webkit-keyframes {{= it.animationName }} {
  0%  {
    -webkit-transform: translateX(0);
  }
  100% {
    -webkit-transform:  translateX({{= it.maxDisplacement }});
  }
}